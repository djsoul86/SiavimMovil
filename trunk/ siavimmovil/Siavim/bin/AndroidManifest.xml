<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dsadsa"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.siavim.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Prueba"
            android:label="Prueba" >
            <intent-filter>
                <action android:name="com.example.dsadsa.Prueba" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ModificarUser"
            android:label="ModificarUser" >
            <intent-filter>
                <action android:name="com.example.dsadsa.ModificarUser" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MostrarCursoActivity"
            android:label="@string/title_activity_mostrar_curso" >
        </activity>
        <activity
            android:name=".Principal_cursos"
            android:label="@string/title_activity_principal_cursos" >
        </activity>
        <activity
            android:name=".MenuCursos"
            android:label="@string/title_activity_menu_cursos" >
        </activity>
    </application>

</manifest>
